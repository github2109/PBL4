package PacDemo;

import java.awt.EventQueue;
import java.awt.Image;
import java.awt.image.BufferedImage;
import java.io.File;
import java.io.IOException;

import javax.imageio.ImageIO;
import javax.swing.ImageIcon;
import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.border.EmptyBorder;
import javax.swing.JLabel;

public class CiscoClone extends JFrame {

	private JPanel contentPane;

	/**
	 * Launch the application.
	 */
	public static void main(String[] args) {
		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					CiscoClone frame = new CiscoClone();
					frame.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

	/**
	 * Create the frame.
	 * @throws IOException 
	 */
	public CiscoClone() throws IOException {
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setBounds(100, 100, 962, 646);
		contentPane = new JPanel();
		contentPane.setBorder(new EmptyBorder(5, 5, 5, 5));

		setContentPane(contentPane);
		contentPane.setLayout(null);
		
		ImageIcon ic3 = new ImageIcon(ClassLoader.getSystemResource("RouterImg.jpg"));
        Image i3 = ic3.getImage().getScaledInstance(250, 250,Image.SCALE_AREA_AVERAGING);
        ImageIcon icc3 = new ImageIcon(i3);
		JLabel picAddRouter = new JLabel(icc3);
		picAddRouter.setBounds(0, 132, 62, 41);
		contentPane.add(picAddRouter);
	}

}
